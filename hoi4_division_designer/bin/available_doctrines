#!/usr/bin/env ruby

require_relative "../lib/division_designer"
require "set"

class AvailableDoctrinesCLI
  def initialize
    @db = Database.new
  end

  def call(args)
    raise "This command takes no arguments" unless args.empty?
    puts @db.doctrines.keys
  end
end

AvailableDoctrinesCLI.new.call(ARGV)
